{
  "name": "@apps/sponza-demo",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "pack-victorian": "mkdir -p build/licensed-victorian && node ./pack-gltf.js assets/licensed-victorian/export/victorian-house.gltf  build/victorian-house/.glb",
    "copy-content": "mkdir -p build/ && rm -fr build/content && cp -fr content/* build/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:production": "yarn copy-content && webpack",
    "build:dev": "yarn copy-content && webpack --config webpack.dev.js",
    "start:dev": "yarn copy-content && webpack --watch --config webpack.dev.js",
    "serve": "yarn copy-content && webpack serve --config webpack.serve.js",
    "clean": "rm -fr build && rm -fr src/**/*.d.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@libs/probes": "^0.0.1",
    "ammojs": "^0.0.2",
    "delaunator": "^5.0.0",
    "lil-gui": "^0.18.2",
    "stats": "^1.0.0",
    "three": "^0.160"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "devDependencies": {
    "@gltf-transform/cli": "^3.10.0",
    "@types/requirejs": "^2.1.32",
    "@types/three": "^0.160",
    "prettier": "^2.5.1",
    "sharp": "^0.33.2",
    "wrap-ansi": "^9.0.0"
  }
}
